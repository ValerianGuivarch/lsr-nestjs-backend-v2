    "@types/node-mailjet": "^3.3.12",
    "@types/streamifier": "^0.1.2",
    "async-lock": "^1.4.1",
    "aws-sdk": "^2.1646.0",
    "bcrypt": "^5.1.1",
    "cache-manager-redis-store": "^3.0.1",
    "chai": "^5.1.1",
    "chance": "^1.1.11",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
