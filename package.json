    "@types/ejs": "^3.1.5",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "20.14.8",
    "@types/pdfkit": "^0.13.4",
    "@types/qs": "^6.9.15",
    "@types/stream-buffers": "^3.0.7",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
