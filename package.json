    "prettier-package-json": "^2.8.0",
    "sonarqube-scanner": "^4.0.1",
    "supertest": "7.0.0",
    "testcontainers": "^10.10.0",
    "ts-jest": "^29.0.0",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "workerIdleMemoryLimit": "1200MB",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "jest-extended/all"
    ]
  },
  "engines": {
    "node": ">=18.11 <=18.16.0",
    "npm": ">=8 <=9.6.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-report.xml",
    "indent": 2
  },
  "lint-staged": {
    "*.(js|ts)": "eslint --fix",
    "*.ts": "prettier --config .prettierrc.json --write src/**/*.ts",
    "package.json": [
      "prettier-package-json --write package.json .eslintrc.json .ncurc.json .prettierrc.json",
      "git add"
    ]
  }
}
